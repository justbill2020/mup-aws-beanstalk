{
  "name": "mup-aws-beanstalk",
  "version": "0.6.2",
  "description": "Deploy apps to AWS Elastic Beanstalk using Meteor Up",
  "main": "index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "babel src --out-dir lib --sourceRoot=./ --copy-files --sourcemaps",
    "build:watch": "npm run build -s -- -w",
    "lint": "npm run lint:code && npm run lint:docs",
    "lint:docs": "node ./tests/validate-docs.js",
    "lint:code": "eslint .",
    "pretest": "npm run lint -s",
    "test": "mocha --require babel-register --require babel-polyfill ./tests/*.spec.js"
  },
  "author": "zodern",
  "license": "MIT",
  "keywords": [
    "meteor-up"
  ],
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "gfm-code-blocks": "^1.0.0",
    "mocha": "^6.2.0",
    "mup": "^1.4.6",
    "shelljs": "^0.8.3"
  },
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "archiver": "^3.0.0",
    "aws-sdk": "^2.500.0",
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.4.2",
    "ejs": "^2.6.2",
    "lodash": "^4.17.15",
    "random-seed": "^0.3.0",
    "shell-escape": "^0.2.0",
    "tar": "^4.4.10",
    "uuid": "^3.3.2"
  }
}
